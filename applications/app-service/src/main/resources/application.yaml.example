server:
  port: 8083
spring:
  application:
    name: "PlazoletaMsMessaging"
  devtools:
    add-properties: false
  h2:
    console:
      enabled: true
      path: "/h2"
  profiles:
    include: null
management:
  endpoints:
    web:
      exposure:
        include: "health,prometheus"
  endpoint:
    health:
      probes:
        enabled: true
cors:
  allowed-origins: "http://localhost:4200,http://localhost:8080"
twilio:
  account:
    sid: ${TWILIO_ACCOUNT_SID:your-account-sid}
  auth:
    token: ${TWILIO_AUTH_TOKEN:your-auth-token}
  phone:
    number: ${TWILIO_PHONE_NUMBER:+1234567890}
